<title>::<​{{ guts }}></title>

<link rel="stylesheet" href="normalize.css">
<link rel="stylesheet" href="turbofish.css">

<t><​{{ guts }}></t>
<u><​{{ guts }}></u>
<r><​{{ guts }}></r>
<b><​{{ guts }}></b>
<o><​{{ guts }}></o>
<f><​{{ guts }}></f>
<i><​{{ guts }}></i>
<s><​{{ guts }}></s>
<h><​{{ guts }}></h>

<script>
	var tailsize = 0;
	var url_path = document.location.pathname.substr(1);
	for(const letter of url_path) {
		if(letter == ':') {
			tailsize += 1;
		} else {
			break;
		}
	}
	const tail = ":".repeat(tailsize);
	for(const l of "turbofish") {
		const e = document.createElement("div");
		e.textContent = tail;
		e.classList.add('fishtail');
		document.querySelector(l).appendChild(e);
}
</script>

